{
  "name": "check-hmrc-api-integration-tests",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test:api": "vitest run --silent=passed-only --select-projects integration-tests/api-gateway --testPathIgnorePatterns */contract/credential-contract.test.ts",
    "test:api:e2e": "vitest run --silent=passed-only integration-tests/api-gateway --testPathIgnorePatterns */contract/credential-contract.test.ts",
    "start:contract": "cd api-gateway/contract && ts-node ./app.ts",
    "test:contract": "wait-on tcp:127.0.0.1:3000; vitest run --silent=passed-only */contract/credential-contract.test.ts --runInBand",
    "test:contract:ci": "npm-run-all -p -r start:contract test:contract",
    "deploy": "../deploy.sh",
    "compile": "tsc"
  },
  "devDependencies": {
    "@aws-sdk/client-cloudformation": "3.828.0",
    "@aws-sdk/client-kms": "3.828.0",
    "@aws-sdk/client-secrets-manager": "3.828.0",
    "@aws-sdk/client-sqs": "3.828.0",
    "@aws-sdk/client-ssm": "3.828.0",
    "@aws-sdk/lib-dynamodb": "3.828.0",
    "@aws-sdk/util-dynamodb": "3.828.0",
    "@pact-foundation/pact": "16.0.0",
    "@types/express": "4.17.21",
    "@types/uuid": "9.0.8",
    "express": "4.21.2",
    "express-asyncify": "2.1.2",
    "jose": "5.1.3",
    "npm-run-all": "4.1.5",
    "testcontainers": "10.23.0",
    "uuid": "8.3.2",
    "wait-on": "8.0.3"
  },
  "dependencies": {
    "@aws-sdk/credential-providers": "3.828.0",
    "aws-sigv4-fetch": "4.4.1"
  },
  "overrides": {
    "form-data": ">=4.0.4"
  }
}
